{
  "name": "babel-plugin-transform-react-jsx-prop-shorthand",
  "version": "0.0.1",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "babel src --out-dir lib --extensions .js,.jsx",
    "prepublishOnly": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "release": "standard-version --skip.commit --skip.tag",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "release:prerelease": "npm run release -- --prerelease",
    "release:prepatch": "npm run release -- --release-as prepatch",
    "release:preminor": "npm run release -- --release-as preminor",
    "release:premajor": "npm run release -- --release-as premajor"
  },
  "keywords": [
    "babel-plugin",
    "jsx",
    "shorthand",
    "react"
  ],
  "author": "KWangechi<https://github.com/KWangechi>",
  "license": "MIT",
  "description": "Transforms React JSX shorthand props with double colons to Standard JSX syntax",
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.9"
  },
  "devDependencies": {
    "vitest": "^0.30.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  }
}
